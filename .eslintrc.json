{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "plugins": ["react"],
  "rules": {
    "import/prefer-default-export": "off",
    "import/extensions": ["error", "always"],
    "linebreak-style": 0
  },
  "settings": {
    "react": {
      "version": "18.2.0"
    },
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
}
